class Solution {
public:
    vector<int> maxSubsequence(vector<int>& nums, int k) {
        int n = nums.size();
        priority_queue<pair<int,int>,vector<pair<int,int>>,greater<pair<int,int>>> pq;
        for (int i=0;i<n;i++){
            pq.push({nums[i],i});
            if (pq.size()>k)pq.pop();
        }
        vector<int> ans;
        while (!pq.empty()){
            auto it = pq.top();
            pq.pop();
            ans.push_back(it.second);
        }
        sort(ans.begin(),ans.end());
        n = ans.size();
        for (int i=0;i<n;i++)ans[i] = nums[ans[i]];
        return ans;
    }
};
